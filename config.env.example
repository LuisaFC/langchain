# Configuração da API do Google Gemini
# Copie este arquivo para .env e preencha com suas chaves reais
GOOGLE_API_KEY=sua_chave_api_gemini_aqui

# Configuração do PostgreSQL com pgvector
PGVECTOR_URL=postgresql://usuario:senha@localhost:5432/nome_do_banco
PGVECTOR_COLLECTION=nome_da_colecao

